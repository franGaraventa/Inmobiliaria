<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://127.0.0.1:3306/inmobiliaria</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        
        <property name="hibernate.show_sql">true</property>
        
        <mapping class="clases.Cliente" package="clases" resource="/clases.Cliente.hbm.xml"></mapping>
        <mapping class="clases.Contrato" package="clases" resource="/clases.Contrato.hbm.xml"></mapping>
        <mapping class="clases.FechaPautada" package="clases" resource="/clases.FechaPautada.hbm.xml"></mapping>
        <mapping class="clases.Imagen" package="clases" resource="/clases.Imagen.hbm.xml"></mapping>
        <mapping class="clases.Locador" package="clases" resource="/clases.Locador.hbm.xml"></mapping>
        <mapping class="clases.Pagos" package="clases" resource="/clases.Pagos.hmb.xml"></mapping>
        <mapping class="clases.Persona" package="clases" resource="/clases.Persona.hbm.xml"></mapping>
        <mapping class="clases.PrecioCompleto" package="clases" resource="/clases.PrecioCompleto.hbm.xml"></mapping>
        <mapping class="clases.PrecioPorcentual" package="clases" resource="/clases.PrecioPorcentual.hbm.xml"></mapping> 
        <mapping class="clases.Propiedad" package="clases" resource="/clases.Propiedad.hbm.xml"></mapping>
        <mapping class="clases.TipoPrecio" package="clases" resource="/clases.TipoPrecio.hbm.xml"></mapping>
        <mapping class="clases.Ubicacion" package="clases" resource="/clases.Ubicacion.hbm.xml"></mapping>
        
        
    </session-factory>
</hibernate-configuration>
